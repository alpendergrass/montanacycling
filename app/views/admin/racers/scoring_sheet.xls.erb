license	road_number	last_name	first_name	city	racing_age	team_name	road_category
<%- for racer in @racers -%>
<%= racer["license"].to_excel -%>
	<%= racer["road_number"] -%>
	<%= racer["last_name"].to_excel -%>
	<%= racer["first_name"].to_excel -%>
	<%= racer["city"].to_excel -%>
	<%= racer["racing_age"] -%>
	<%= racer["team_name"].to_excel -%>
